    "def my_mul(*args):\n",
    "    if len(args)==1:\n",
    "        return args[0]\n",
    "    else:\n",
    "        result=1\n",
    "        for i in range(len(args)):\n",
    "            result = result*args[i]\n",
    "    return result"
   ]
